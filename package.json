{
  "name": "electron-builder-app",
  "productName": "electron-builder-app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "pub": "electron-builder --publish always"
  },
  "keywords": [],
  "author": {
    "name": "Kamil Samaj",
    "email": "kamilsamaj@gmail.com"
  },
  "license": "MIT",
  "build": {
    "appId": "com.github.kamilsamaj.electron-builder-app",
    "productName": "Electron Builder App",
    "copyright": "MIT license",
    "directories": {
      "output": "release/"
    },
    "linux": {
      "icon": "dist",
      "target": [
        "zip",
        "deb"
      ],
      "category": "utility"
    },
    "publish": {
      "provider": "s3",
      "bucket": "kamils-electron-app",
      "path": "app"
    }
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_builder_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.67",
    "@electron-forge/maker-deb": "^6.0.0-beta.67",
    "@electron-forge/maker-rpm": "^6.0.0-beta.67",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.67",
    "@electron-forge/maker-zip": "^6.0.0-beta.67",
    "electron": "21.1.1",
    "electron-builder": "^23.6.0"
  }
}
